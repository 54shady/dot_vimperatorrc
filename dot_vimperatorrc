source! /home/zeroway/.vimperatorrc.local

set runtimepath=/home/zeroway/.vimperator
let mapleader = ","

"buffer
nmap <leader>ls :ls<CR>
nmap <leader>bd :bd<CR>
nmap <leader>bn :bn<CR>
nmap <leader>bp :bp<CR>

hi Hint::after -append font-size:15pt

" vim: set ft=vimperator:
